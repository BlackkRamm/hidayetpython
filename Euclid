import math

def euclidean_distance(point1, point2):
    x1, y1 = point1
    x2, y2 = point2
    return math.sqrt((x2 - x1)**2 + (y2 - y1)**2)

# İki noktanın koordinatlarını kullanarak minimum Öklid mesafesini hesaplayalım
point1 = (1, 2)
point2 = (4, 6)
distance = euclidean_distance(point1, point2)

print("İki nokta arasındaki minimum Öklid mesafesi:", distance)
